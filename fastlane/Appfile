# Fastlane Appfile for FortDocs
# This file contains app-specific configuration for Fastlane

# App Identifier
app_identifier("com.fortdocs.app")

# Apple Developer Account
apple_id(ENV["APPLE_ID"])

# Team ID (can be found in Apple Developer Portal)
team_id(ENV["TEAM_ID"])

# iTunes Connect Team ID (if different from Developer Team ID)
itc_team_id(ENV["ITC_TEAM_ID"])

# For more information about the Appfile, see:
# https://docs.fastlane.tools/advanced/#appfile

